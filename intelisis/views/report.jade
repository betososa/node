extends layout

block content
	div
		table.table
			caption Resumen de las acciones
			thead
				tr
					for i in data
						for j in i.resource
							th= j.name
			tbody
				tr
					for i in data
						for j in i.resource
							th= j.price
				tr
					for i in data
						for j in i.resource
							th= j.symbol
				tr
					for i in data
						for j in i.resource
							th= j.ts
				tr
					for i in data
						for j in i.resource
							th= j.type
				tr
					for i in data
						for j in i.resource
							th= j.utctime
				tr
					for i in data
						for j in i.resource
							th= j.volume

- console.log(JSON.stringify(locals))
- console.log("JSON.stringify(data)")



